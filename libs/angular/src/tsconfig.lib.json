{
  "compilerOptions": {
    "declaration": true,
    "module": "es2015",
    "target": "es5",
    "baseUrl": ".",
    "stripInternal": true,
    "emitDecoratorMetadata": false,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "outDir": "../../dist/out-tsc",
    "rootDir": ".",
    "lib": [
      "es2016",
      "es2017",
      "dom"
    ],
    "skipLibCheck": true,
    "types": []
  },
  "angularCompilerOptions": {
    "enableIvy": true,
    "annotateForClosureCompiler": false,
    "strictMetadataEmit": true,
    "skipTemplateCodegen": true,
    "flatModuleOutFile": "fds.js",
    "flatModuleId": "fds"
  },
  "files": [
    "./public_api.ts"
  ]
}
